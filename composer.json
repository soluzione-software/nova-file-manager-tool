{
    "name": "soluzione-software/nova-file-manager-tool",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/view": "5.8.*||^6.0||^7.0||^8.0",
        "laravel/nova": "~2.0||~3.0",
        "illuminate/routing": "5.8.*||^6.0||^7.0||^8.0"
    },
    "autoload": {
        "psr-4": {
            "SoluzioneSoftware\\Nova\\Tools\\FileManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoluzioneSoftware\\Nova\\Tools\\FileManager\\ToolServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
